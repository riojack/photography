    "bootstrap:dev_setup": ". $NVM_DIR/nvm.sh && nvm install -lts=boron; nvm use --lts=boron",
    "bootstrap:first_run": "npm run bootstrap:cleanup && npm i && npm run build",
    "bootstrap:server": "node server.js",
    "bootstrap:cleanup": "rimraf dist && rimraf node_modules",
    "build": "npm test && npm run webpack",
    "webpack": "webpack -p",
    "test": "mocha test --recursive",
    "bump:patch": "npm version patch -m \"Pre-release, as %s.\"",
